  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Game",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/src/tstt_rl.py",
      "console": "integratedTerminal"
    },
    {
      "name": "Python: Debug Pytest",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/.venv/bin/pytest",
      "args": ["--maxfail=1", "--disable-warnings"],
      "console": "integratedTerminal"
    }
  ]
}
